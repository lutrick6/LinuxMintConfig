alias install='sudo apt install -yf'
alias atom='snap run atom'
alias c='clear && . ~/.bashrc'
alias repo='sudo add-apt-repository -y'
alias alien='~/.bash_aliases'
alias update='sudo apt update'
alias uninstall='sudo apt remove --purge'
alias fsupdate='sudo apt update && sudo apt upgrade -y && sudo apt full-upgrade -y && sudo apt autoremove -y'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias startplex='sudo systemctl start plexmediaserver'
alias get-users='cut -d: -f1 /etc/passwd'
alias django_env='source /home/ilucid/Dev/Environments/django_env/bin/activate'
alias panda_env='source /home/ilucid/Dev/Environments/panda_env/bin/activate'
alias newpythonfile='atom /home/ilucid/Dev/Python/$(date +'%m_%d_%Y').py'
